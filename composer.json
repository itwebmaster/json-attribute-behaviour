{
  "name": "itwebmaster/json-attribute-behavior",
  "description": "Yii2 trait for easy JSON attribute handling with schema validation",
  "type": "library",
  "autoload": {
    "psr-4": {
      "itwebmaster\\JsonAttributeBehavior\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": "^2.0.38",
    "yidas/yii2-composer-bower-skip": "^2.0"
  },
  "config": {
    "process-timeout": 1800,
    "preferred-install": "dist",
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": [
      "yidas\\yii2\\composer\\bower\\BowerSkipInstaller::postCreateProject"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
