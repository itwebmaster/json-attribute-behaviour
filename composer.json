{
  "name": "itwebmaster/json-attribute-behavior",
  "description": "Flexible Yii2 trait for working with JSON fields (dot-paths, validation schema, defaults, type casting, etc.)",
  "keywords": ["yii2", "json", "behavior", "attribute", "schema", "cast", "trait"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/itwebmaster/json-attribute-behavior/issues",
    "source": "https://github.com/itwebmaster/json-attribute-behavior"
  },
  "authors": [
    {
      "name": "Ivan Tymchyna",
      "email": "it.web.master@icloud.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": "^2.0.38",
    "yidas/yii2-composer-bower-skip": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": { "itwebmaster\\JsonAttributeBehavior\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "tests\\": "tests/" }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "process-timeout": 1800,
    "preferred-install": "dist",
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
